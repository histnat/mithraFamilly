package com.sonovision.openconv.ws.controller.model;

import com.sonovision.openconv.wf.model.ParameterId;
import com.sonovision.openconv.wf.model.ParameterTypes;

public class TaskParameterModel {

	private ParameterId id;//taskId_parameterCode
	private String label;
	private String value;
	private String code;
	private ParameterTypes type;
	private String descr;
	public String getLabel() {
		return label;
	}
	public void setLabel(String label) {
		this.label = label;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public ParameterTypes getType() {
		return type;
	}
	public void setType(ParameterTypes type) {
		this.type = type;
	}
	public String getDescr() {
		return descr;
	}
	public void setDescr(String descr) {
		this.descr = descr;
	}
	public ParameterId getId() {
		return id;
	}
	public void setId(ParameterId id) {
		this.id = id;
	}
	
	
	
}
