<supequis id="O2.C0038"><refipc csn="72016002 015 "><nomen>Cliquet r&eacute;versible carr&eacute; &frac14;</nomen><quantity>1</quantity></supequis>
<supequis id="O2.C0048"><refipc csn="72016002 058 "><nomen>Cliquet r&eacute;versible carr&eacute; &frac12;</nomen><quantity>1</quantity></supequis>
<supequis id="O2.S0551"><refipc csn="72016001 032 "><nomen>Elingue rallonge grande&nbsp;700&nbsp;mm</nomen><quantity>2</quantity></supequis>
<supequis id="O2.S0680"><refipc csn="72016001 029 "><nomen>Cl&eacute; dynamom&eacute;trique R302DA 1-5&nbsp;N.m</nomen><quantity>1</quantity></supequis>
