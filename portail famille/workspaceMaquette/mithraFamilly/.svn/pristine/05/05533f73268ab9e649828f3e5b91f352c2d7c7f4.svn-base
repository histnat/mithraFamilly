<!-- DOCTYPE em   [   -->

<!-- The following set of declarations may be referred to
     using the public entity as follows:
<!DOCTYPE em PUBLIC
    "-//ATA-HONEYWELL-TEXT//DTD EM-VER6-REV2-LEVEL2//EN" [ ]>          -->

<!-- ======================================================= -->
<!--          NOTATIONS                                      -->
<!-- ======================================================= -->

<!NOTATION cgm    PUBLIC "-//USA-DOD//NOTATION Computer Graphics Metafile//EN"          >
<!NOTATION ccitt4 PUBLIC "-//USA-DOD//NOTATION CCITT Group4 Facsimile//EN"              >
<!NOTATION gcf    PUBLIC "-//ATA-TEXT//NOTATION Graphic Companion File//EN"             >
<!NOTATION pdf    PUBLIC "-//Adobe Inc.//NOTATION Portable Document Format//EN"         >
<!NOTATION png    PUBLIC "-//W3C//NOTATION Portable Network Graphics//EN"               >
<!NOTATION jpeg   PUBLIC "+//ISBN 0-7923-9432-1::Graphic Notation//NOTATION Joint Photographic Experts Group Raster//EN" >
<!NOTATION vrml   PUBLIC  "-//ATA-TEXT//NOTATION Virtual Reality Modeling Language//EN" >
<!NOTATION mpeg   PUBLIC "-//ATA-TEXT//NOTATION Motion Pictures Expert Group//EN"       >
<!NOTATION mp3    PUBLIC "-//ATA-TEXT//NOTATION Motion Pictures Expert Group, Audio Layer 3//EN" >

<!-- ======================================================  -->
<!--                      ENTITIES                           -->
<!-- ======================================================= -->

<!ENTITY % number  'CDATA'                                     >
<!ENTITY % nutoken 'CDATA'                                     >
<!ENTITY % name    'CDATA'                                     >

<!ENTITY  % g.r  "(refblock* | ((grphcref | gxrefeff)*, refext*, refint*))" >
<!ENTITY  % w.c      "(warning*, caution*)"                    >
<!ENTITY  % text-no-note  "para | unlist | numlist | table        | list1" >
<!ENTITY  % text-no-table "para | unlist | numlist |         note | list1" >
<!ENTITY  % text-no-list1 "para | unlist | numlist | table               " >
<!ENTITY  % text          "para | unlist | numlist | table | note | list1" >
<!ENTITY  % deleted  "(deleted, chgdesc*)"                     >
<!ENTITY  % yesorno  "%number;"                                >
<!ENTITY  % revatt
            "chg       (N|D|R|U|n|d|r|u)          #REQUIRED
             revdate    %number;                  #REQUIRED
             key        ID                        #REQUIRED"   >

<!ENTITY % ISOtech PUBLIC "ISO 8879-1986//ENTITIES General Technical//EN"
'entities\iso-tech.ent' >
<!ENTITY % ISOpub  PUBLIC "ISO 8879-1986//ENTITIES Publishing//EN"
'entities\iso-pub.ent' >
<!ENTITY % ISOnum  PUBLIC "ISO 8879-1986//ENTITIES Numeric and Special Graphic//EN"
'entities\iso-num.ent' >
<!ENTITY % ISOgrk1 PUBLIC "ISO 8879-1986//ENTITIES Greek Letters//EN"
'entities\iso-grk1.ent' >
<!ENTITY % ISOgrk2 PUBLIC "ISO 8879-1986//ENTITIES Monotoniko Greek//EN"
'entities\iso-grk2.ent' >
<!ENTITY % ISOgrk3 PUBLIC "ISO 8879-1986//ENTITIES Greek Symbols//EN"
'entities\iso-grk3.ent' >
<!ENTITY % ISOgrk4 PUBLIC "ISO 8879-1986//ENTITIES Alternative Greek Symbols//EN"
'entities\iso-grk4.ent' >
<!ENTITY % ISOlat1 PUBLIC "ISO 8879-1986//ENTITIES Added Latin 1//EN"
'entities\iso-lat1.ent' >
<!ENTITY % ISObox  PUBLIC "ISO 8879-1986//ENTITIES Box and Line Drawing//EN"
'entities\iso-box.ent' >
<!ENTITY % ISOamso PUBLIC "ISO 8879-1986//ENTITIES Added Math Symbols: Ordinary//EN"
'entities\iso-amso.ent' >
<!ENTITY % ISOamsc PUBLIC "ISO 8879-1986//ENTITIES Added Math Symbols: Delimiters//EN"
'entities\iso-amsc.ent' >

%ISOtech; %ISOpub; %ISOnum; %ISOgrk1; %ISOgrk2; %ISOgrk3; %ISOgrk4; %ISOlat1; 
%ISObox; %ISOamso; %ISOamsc;


<!--                                                         -->
<!-- ======================================================  -->
<!--                   EM TOP LEVEL STRUCTURE                -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT em            ((title, partinfo*, legalntc?, mfmatr, chapter*)
                        | increv | tr+)                        >
<!ATTLIST em
          spl       CDATA                         #REQUIRED
          prtnrspl  CDATA                         #IMPLIED
          model     CDATA                         #REQUIRED
          oidate    %number;                      #REQUIRED
          tsn       CDATA                         #REQUIRED
          cus       CDATA                         #IMPLIED
          cusname   CDATA                         #IMPLIED
          docnbr    CDATA                         #REQUIRED
          lang      CDATA                         #REQUIRED
          type      (em | lmm | hmm | emm | eohm | spm | rm) "em"
          revdate   %number;                      #REQUIRED
          chg       (N|R|U|n|r|u)                 "N"
          efflen    %number;                      #IMPLIED
          efftype   CDATA                         #IMPLIED
          overlayformatstyle (draft | final | preliminary | validation)
                                                  "final"
          edit      (xml | sgml)                  "sgml"       >


<!ELEMENT increv         (transltr?, legalntc?, (chapter
                          | section | subject | pgblk | trlist
                          | conindex | sblist | intro | howtouse
                          | hazmtlst)+)                        >
<!--                                                         -->
<!-- ======================================================= -->
<!-- A question for future consideration:                    -->
<!-- should we remove task, sheet and graphic, from "tr"     -->
<!-- since we are making pgblk the lowest anchor in increv?  -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT tr             (trfmatr, (chapter | section+
                         | subject+ | pgblk+ | task+ | sheet+
                         | graphic+ | intro | sblist | legalntc
                         | hazmtlst | subtask+))               >

<!ELEMENT trfmatr        (title, trreason, (%text;)*)          >
<!ATTLIST trfmatr
          trnbr     CDATA                         #REQUIRED
          issdate   %number;                      #REQUIRED
          trdel     CDATA                         #IMPLIED
          trrepl    CDATA                         #IMPLIED     >

<!ELEMENT trreason       (%text;)+                             >

<!-- ======================================================= -->
<!--       Part Information                          -->
<!-- ======================================================= -->
<!ELEMENT partinfo  (title?, (cmpnom?, mfrpnr)+)                 >
<!ATTLIST partinfo
          model     CDATA                            #IMPLIED    >

<!ELEMENT cmpnom    (#PCDATA)                                    >
<!ELEMENT mfrpnr    (pnr, mfr, altpnr*)                          >
<!ELEMENT mfr       (#PCDATA)                                    >
<!ELEMENT altpnr    (pnr, mfr)                                   >

<!-- ======================================================= -->
<!--       Legal Notice Information                          -->
<!-- ======================================================= -->

<!ELEMENT legalntc      (exprtcl | cpyrght | proptary | geninfo)+>
<!ATTLIST legalntc
          %revatt;                                             >

<!ELEMENT proptary      (fullstmt, partstmt?)                  >
<!ELEMENT fullstmt      (title?, cpyrght?, para+)              >
<!ELEMENT partstmt      (title?, para+)                           >
<!ELEMENT exprtcl       (para | unlist | numlist | note)+      >
<!ELEMENT cpyrght       (year*, holder*, geninfo)              >
<!ELEMENT year          (#PCDATA)                              >
<!ELEMENT holder        (#PCDATA)                              >
<!ELEMENT geninfo       (para | unlist | numlist | note)+      >

<!-- ======================================================  -->
<!--             ENGINE MANUAL FRONT MATTER                  -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT mfmatr        (transltr?,  trlist?, conindex?,
                         sblist*, intro?, howtouse?, hazmtlst?)>
                         
<!ELEMENT transltr      (chgdesc*, title, (%text;)+)          >
<!ATTLIST transltr
          oldkey    %name;                        #IMPLIED
          %revatt;                                             >

<!ELEMENT trlist        (trdata+ | isempty)                    >
<!ATTLIST trlist
          oldkey    %name;                        #IMPLIED
          %revatt;                                             >

<!ELEMENT trdata        (effect?, trnbr, trstatus, trloc+)     >

<!ELEMENT trnbr         (#PCDATA)                              >
<!ELEMENT trstatus      (#PCDATA)                              >
<!ELEMENT trloc         (#PCDATA)                              >

<!ELEMENT conindex      (chgdesc*, title, (prclist1 | table)+)+>
<!ATTLIST conindex
          oldkey    %name;                        #IMPLIED
          %revatt;                                             >

<!ELEMENT sblist        (effect?, chgdesc*, title, (%text;)*,
                        (sbdata+ | isempty))                   >
<!ATTLIST sblist
          %revatt;                                             >

 <!ELEMENT sbdata        (sbnbr, sbdesc, issdate, ics)        >

<!ELEMENT intro         (chgdesc*, title, prclist1)+           >
<!ATTLIST intro
          oldkey    %name;                        #IMPLIED
          %revatt;                                             >

<!ELEMENT howtouse      (chgdesc*, title, prclist1)+           >
<!ATTLIST howtouse
          oldkey    %name;                        #IMPLIED
          %revatt;                                             >

<!ELEMENT sbnbr         (#PCDATA)                              >
<!ELEMENT issdate       (#PCDATA)                              >
<!ELEMENT ics           (#PCDATA)                              >
<!ELEMENT sbdesc        (#PCDATA)                              >

<!ELEMENT hazmtlst      (title, pretopic*, hazmt+)             >
<!ATTLIST hazmtlst
          %revatt;                                             >
<!ELEMENT pretopic      (effect?, title, (%text;)+ )              >
<!ELEMENT hazmt         (title, warning)                       >
<!ATTLIST hazmt
          id        ID                            #REQUIRED    >
          
<!--                                                         -->
<!-- ======================================================  -->
<!--                      CHAPTER                            -->
<!-- ======================================================  -->
<!--                                                         -->
<!ELEMENT chapter        (effect?, ((chgdesc*, title, section+)
                         | %deleted;))                         >
<!ATTLIST chapter
          chapnbr   %number;                      #REQUIRED
          %revatt;                                             >
<!--                                                         -->
<!-- ======================================================= -->
<!--                      SECTION                            -->
<!-- ======================================================  -->
<!--                                                         -->
<!ELEMENT section        (effect?, ((chgdesc*,title, subject+)
                         | %deleted;))                         >
<!ATTLIST section
          chapnbr   %number;                      #REQUIRED
          sectnbr   %number;                      #REQUIRED
          %revatt;                                             >
<!--                                                         -->
<!-- ======================================================= -->
<!--                       SUBJECT                           -->
<!-- ======================================================  -->
<!--                                                         -->
<!ELEMENT subject        (effect?, ((chgdesc*, title, pgblk+)
                         | %deleted;))                         >
<!ATTLIST subject
          chapnbr   %number;                      #REQUIRED
          sectnbr   %number;                      #REQUIRED
          subjnbr   %number;                      #REQUIRED
          append  (N|Y)                               "N"
          %revatt;                                             >
<!--                                                         -->
<!-- ======================================================  -->
<!--                      PAGEBLOCK                          -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT pgblk          (effect?, ((chgdesc*, title, task+)
                         | %deleted;))                         >
<!ATTLIST pgblk
          chapnbr   %number;                      #REQUIRED
          sectnbr   %number;                      #REQUIRED
          subjnbr   %number;                      #REQUIRED
          pgblknbr  %number;                      #REQUIRED
          breaknbr  CDATA                         "NA"
          confnbr   CDATA                         "NA"
          confgtxt  CDATA                         ""
          %revatt;                                             >

<!--                                                         -->
<!-- ======================================================= -->
<!--                        TASK                             -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT task           (effect?, ((chgdesc*, title, (%g.r;),
                         (%w.c;), note*, (subtask | graphic)*) | %deleted;)) >
<!ATTLIST task
          chapnbr   %number;                      #REQUIRED
          sectnbr   %number;                      #REQUIRED
          subjnbr   %number;                      #REQUIRED
          pgblknbr  %number;                      #REQUIRED
          breaknbr  CDATA                         "NA"
          confnbr   CDATA                         "1"
          confgtxt  CDATA                         ""
          func      CDATA                         #REQUIRED
          seq       CDATA                         #REQUIRED
          confltr   CDATA                         "A"
          varnbr    CDATA                         "01"
          alunqi    CDATA                         ""
          %revatt;                                             >


<!-- ======================================================= -->
<!--                      SUBTASK                            -->
<!-- ======================================================= -->
<!-- Changed "varnbr" from NUMBER to CDATA to accommodate    -->
<!-- non-numeric legacy data of some manufacturers.          -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT subtask        (effect*, ((chgdesc*, title, (%g.r;), (%w.c;), note*, (list1 | table*)) | %deleted;)) >
<!ATTLIST subtask
          chapnbr   %number;                      #REQUIRED
          sectnbr   %number;                      #REQUIRED
          subjnbr   %number;                      #REQUIRED
          pgblknbr  %number;                      #REQUIRED
          func      CDATA                         #REQUIRED
          seq       CDATA                         #REQUIRED
          confltr   CDATA                         "a"
          varnbr    CDATA                         "01"
          alunqi    CDATA                         ""
          confnbr   CDATA                         "1"
          %revatt;                                             >

<!--                                                         -->
<!-- ======================================================= -->
<!--             PROCEDURE LIST STRUCTURE      (NON-TASKED)  -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT prclist1       (effect?, prcitem1+)                  >
<!ELEMENT prclist2       (effect?, prcitem2+)                  >
<!ELEMENT prclist3       (effect?, prcitem3+)                  >
<!ELEMENT prclist4       (effect?, prcitem4+)                  >
<!ELEMENT prclist5       (effect?, prcitem5+)                  >
<!ELEMENT prclist6       (effect?, prcitem6+)                  >
<!ELEMENT prclist7       (effect?, prcitem7+)                  >

<!ELEMENT prcitem1       (prcitem, prclist2?)                  >
<!ATTLIST prcitem1
          id       ID                             #IMPLIED     >
<!ELEMENT prcitem2       (prcitem, prclist3?)                  >
<!ATTLIST prcitem2
          id       ID                             #IMPLIED     >
<!ELEMENT prcitem3       (prcitem, prclist4?)                  >
<!ATTLIST prcitem3
          id       ID                             #IMPLIED     >
<!ELEMENT prcitem4       (prcitem, prclist5?)                  >
<!ATTLIST prcitem4
          id       ID                             #IMPLIED     >
<!ELEMENT prcitem5       (prcitem, prclist6?)                  >
<!ATTLIST prcitem5
          id       ID                             #IMPLIED     >
<!ELEMENT prcitem6       (prcitem, prclist7?)                  >
<!ATTLIST prcitem6
          id       ID                             #IMPLIED     >
<!ELEMENT prcitem7       (prcitem)                             >
<!ATTLIST prcitem7
          id       ID                             #IMPLIED     >

<!ELEMENT prcitem        (title?, (%w.c;)?, (%text-no-list1;), note*, graphic*)>

<!--                                                         -->
<!-- ======================================================= -->
<!--                STANDARD LISTS                           -->
<!-- ======================================================= -->
<!-- The "cont" attribute existed in version 2.2, but this   -->
<!-- comment is added for explanation.  The "cont" attribute -->
<!-- was added to lists (listN) and list items (LNitem) to   -->
<!-- control step numbers, so that step numbers could        -->
<!-- continue across subtasks, instead of starting with "1"  -->
<!-- each time a subtask occurred in a list.  The numbering  -->
<!-- continues from the previous list or list item if the    -->
<!-- value of "cont" is "1".  The numbering restarts at the  -->
<!-- appropriate initial value ("1" or "A"), if the value of -->
<!-- "cont" is "0".                                          -->
<!-- ======================================================= -->
<!-- Added attribute "alt" to enable two numbering schemes   -->
<!-- to be used as visual cues to alternative elements below -->
<!-- the task or subtask level.  Example: steps 1, 2, 3 and  -->
<!-- steps 1A, 2A, 3A.                                       -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT list1          (effect?, l1item+)                    >
<!ELEMENT list2          (effect?, l2item+)                    >
<!ELEMENT list3          (effect?, l3item+)                    >
<!ELEMENT list4          (effect?, l4item+)                    >
<!ELEMENT list5          (effect?, l5item+)                    >
<!ELEMENT list6          (effect?, l6item+)                    >
<!ELEMENT list7          (effect?, l7item+)                    >

<!ELEMENT l1item        (effect*, (%w.c;), (%text-no-list1;), note*, (list2, note*)?)  >
<!ELEMENT l2item        (effect*, (%w.c;), (%text-no-list1;), note*, (list3, note*)?)  >
<!ELEMENT l3item        (effect*, (%w.c;), (%text-no-list1;), note*, (list4, note*)?)  >
<!ELEMENT l4item        ((%w.c;), (%text-no-list1;), note*, (list5, note*)?)  >
<!ELEMENT l5item        ((%w.c;), (%text-no-list1;), note*, (list6, note*)?)  >
