<!ELEMENT l7item        ((%w.c;), (%text-no-list1;), note*)  >


<!-- ======================================================= -->
<!--            TABLE (CELLULAR, CALS BASED)                 -->
<!-- ======================================================= -->
<!-- Added attribute "alt" to enable two numbering schemes   -->
<!-- to be used as visual cues to alternative elements below -->
<!-- the task or subtask level.  Example: steps 1, 2, 3 and  -->
<!-- steps 1A, 2A, 3A.                                       -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT table          ((title?, tgroup, ftnote*) | graphic+) >
<!ATTLIST table
          tabstyle  NMTOKEN                        #IMPLIED
          frame     (top | bottom | topbot | all | sides | none)
                                                   #IMPLIED
          colsep    %yesorno;                      #IMPLIED
          rowsep    %yesorno;                      #IMPLIED
          orient    (port | land)                  #IMPLIED
          pgwide    %yesorno;                      #IMPLIED
          id        ID                             #IMPLIED
          formal    (F | I)                        "I"         >

<!ELEMENT tgroup         (colspec*, spanspec*, thead?, tfoot?, tbody) >
<!ATTLIST tgroup
          cols      %number;                       #REQUIRED
          colsep    %yesorno;                      #IMPLIED
          rowsep    %yesorno;                      #IMPLIED
          align     (left | right | center | justify | char)
                                                   "left"
          charoff   %nutoken;                      "50"
          char      CDATA                          ""          >

<!ELEMENT colspec        EMPTY                                 >
<!ATTLIST colspec
          colnum    %number;                       #IMPLIED
          colname   NMTOKEN                        #IMPLIED
          align     (left | right | center | justify | char)
                                                   #IMPLIED
          charoff   %nutoken;                      #IMPLIED
          char      CDATA                          #IMPLIED
          colwidth  CDATA                          #IMPLIED
          colsep    %yesorno;                      #IMPLIED
          rowsep    %yesorno;                      #IMPLIED    >

<!ELEMENT spanspec       EMPTY                                 >
<!ATTLIST spanspec
          namest    NMTOKEN                        #REQUIRED
          nameend   NMTOKEN                        #REQUIRED
          spanname  NMTOKEN                        #IMPLIED
          align     (left | right | center | justify | char)
                                                   "center"
          charoff   %nutoken;                      #IMPLIED
          char      CDATA                          #IMPLIED
          rowsep    %yesorno;                      #IMPLIED
          colsep    %yesorno;                      #IMPLIED    >

<!ELEMENT thead          (colspec*, row+)                      >
<!ATTLIST thead
          valign   (top|middle|bottom)             "bottom"    >

<!ELEMENT tfoot          (colspec*, row+)                      >
<!ATTLIST tfoot
          valign   (top|middle|bottom)             "top"       >

<!ELEMENT tbody          (row+)                                >
<!ATTLIST tbody
          valign   (top|middle|bottom)             "top"       >

<!ELEMENT row            (entry+)                              >
<!ATTLIST row
          rowsep    %yesorno;                      #IMPLIED    >

<!ELEMENT entry          (effect*, (%text-no-table; | graphic | %w.c;))*  >
<!ATTLIST entry
          colname   NMTOKEN                        #IMPLIED
          namest    NMTOKEN                        #IMPLIED
          nameend   NMTOKEN                        #IMPLIED
          spanname  NMTOKEN                        #IMPLIED
          morerows  %number;                       "0"
          colsep    %yesorno;                      #IMPLIED
          rowsep    %yesorno;                      #IMPLIED
          rotate    %yesorno;                      "0"
          valign    (top | bottom | middle)        "top"
          align     (left | right | center | justify | char)
                                                   #IMPLIED
          charoff   %nutoken;                      #IMPLIED
          char      CDATA                          #IMPLIED    >

<!ELEMENT ftnote         (%text-no-table;)+                    >
<!ATTLIST ftnote
          ftnoteid  ID                             #REQUIRED   >
<!--                                                         -->
<!-- ======================================================= -->
<!--                         GRAPHICS                        -->
<!-- ======================================================= -->
<!-- seq attribute change from NUMBER to CDATA and all       -->
<!-- attributes changed to #IMPLIED so graphic can be used   -->
<!-- in front matter, where chapnbr, etc. is not applicable. -->
<!-- ======================================================= -->
<!-- Changed "varnbr" from NUMBER to CDATA to accommodate    -->
<!-- legacy data of some manufacturers.                      -->
<!--                                                         -->
<!-- Added attribute "alt" to enable two numbering schemes   -->
<!-- to be used as visual cues to alternative elements below -->
<!-- the task or subtask level.  Example: steps 1, 2, 3 and  -->
<!-- steps 1A, 2A, 3A.                                       -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT graphic        (effect?, ((chgdesc*, title?, sheet+)
                         | %deleted;))                         >
<!ATTLIST graphic
          chapnbr   %number;                      #REQUIRED
          sectnbr   %number;                      #REQUIRED
          subjnbr   %number;                      #REQUIRED
          pgblknbr  %number;                      #REQUIRED
          func      CDATA                         #REQUIRED
          seq       CDATA                         #REQUIRED
          confltr   CDATA                         "a"
          varnbr    CDATA                         "01"
          alunqi    CDATA                         ""
          %revatt;                                             >

<!ELEMENT sheet          (effect?, (chgdesc*, title?,
                         gdesc?) | (%deleted;)?)                 >
<!ATTLIST sheet
          gnbr      ENTITY                        #REQUIRED
          cfnbr     ENTITY                        #IMPLIED
          sheetnbr  CDATA                         #REQUIRED
          imgarea  (AP|BP|CP|DL|EL|FP|GP|HL|IL
                   |ap|bp|cp|dl|el|fp|gp|hl|il)   #REQUIRED
          %revatt;                                             >

<!ELEMENT gdesc          (effect?, (%text;)+)                  >

<!ELEMENT gxrefeff       (effect, grphcref)                    >
<!ELEMENT grphcref       (#PCDATA)                             >
<!ATTLIST grphcref
          refid     IDREF                         #REQUIRED
          sheetnbr  CDATA                         #IMPLIED
          structid  CDATA                         #IMPLIED
          shownow   %yesorno;                     "0"          >

<!ELEMENT grsymbol        EMPTY                                >
<!ATTLIST grsymbol
          gnbr          ENTITY                    #REQUIRED
          reprowid      CDATA                     #IMPLIED
          reprohgt      CDATA                     #IMPLIED
          reproscl      CDATA                     #IMPLIED     >

<!ELEMENT refblock      (#PCDATA | refint | refext | grphcref | gxrefeff)*>
<!--                                                         -->
<!-- ======================================================= -->
<!--             NUMBERED AND UN-NUMBERED LISTS              -->
<!-- ======================================================  -->
<!--                                                         -->
<!ELEMENT unlist         (unlitem+)                            >
<!ATTLIST unlist
          bulltype  (NONE | BULLET | NDASH | MDASH | SQUARE | DIAMOND | ASTERISK | DELTA | SYSTEM
                    |none | bullet | ndash | mdash | square | diamond | asterisk | delta | system)
                                                  "system"     >

<!ELEMENT unlitem        (para+)                               >

<!ELEMENT numlist        (numlitem+)                           >
<!ATTLIST numlist
          numtype   (NNP | AUP | NNB | ALB | NNS | ALS | DBR | RUP | RLP | RUR | RLR | NNR | AUR | ALR
                    |nnp | aup | nnb | alb | nns | als | dbr | rup | rlp | rur | rlr | nnr | aur | alr)
                                                  "nnp"        >

<!ELEMENT numlitem       (para+)                               >
<!ATTLIST numlitem
          label     CDATA                         #IMPLIED     >
<!--                                                         -->
<!-- ======================================================= -->
<!--                       PARAGRAPH                         -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT para          (#PCDATA | con | csn | equ | grphcref | gxrefeff
                        | ncon | pnr | refext | refint | std
                        | sub | super | ted | txtgrphc
                        | refmedia | grsymbol |  acro | abbr)*                >
<!--                                                         -->
<!-- ======================================================= -->
<!--                  PARAGRAPH CONTENTS                     -->
<!-- ======================================================= -->
<!--                                                         -->
<!ELEMENT con           (connbr, conname, condesc?, consrc)    >
<!ELEMENT connbr        (#PCDATA)                              >
<!ELEMENT conname       (#PCDATA |  acro | abbr)*              >
<!ELEMENT condesc       (#PCDATA |  acro | abbr)*              >
<!ELEMENT consrc        (#PCDATA)                              >

<!ELEMENT csn           EMPTY                                  >
<!ATTLIST csn
          chapnbr   %number;                      #REQUIRED
          sectnbr   %number;                      #REQUIRED
          unitnbr   %number;                      #REQUIRED
          fignbr    %nutoken;                     #REQUIRED
          itemnbr   %nutoken;                     #REQUIRED    >

<!ELEMENT equ           (#PCDATA | sub | super)*               >
<!ELEMENT ncon          (#PCDATA)                              >
<!ELEMENT pnr           (#PCDATA)                              >

<!ELEMENT refext    (#PCDATA | acro | abbr)*                     >
<!ATTLIST refext
          refman    CDATA                         #IMPLIED
          docnbr    CDATA                         #IMPLIED
          refloc    CDATA                         #IMPLIED
          refspl    CDATA                         #IMPLIED
          refmodel  CDATA                         #IMPLIED     >

<!ELEMENT refint        (#PCDATA)                              >
<!ATTLIST refint
          reftype   (trlist | pgblk | task | subtask | table | other)  "other"
          refid     IDREF                         #REQUIRED    >

<!ELEMENT refmedia     (#PCDATA)                               >
<!ATTLIST refmedia
          mednbr    ENTITY                        #REQUIRED    >


<!ELEMENT std           (stdnbr, stdname, stddesc?, stdsrc)    >
<!ELEMENT stdnbr        (#PCDATA |  acro | abbr)*              >
<!ELEMENT stdname       (#PCDATA |  acro | abbr)*              >
<!ELEMENT stddesc       (#PCDATA |  acro | abbr)*              >
<!ELEMENT stdsrc        (#PCDATA)                              >
<!ELEMENT sub           (#PCDATA)                              >
<!ELEMENT super         (#PCDATA)                              >
<!ELEMENT ted           (toolnbr, toolname, tooldesc?, toolsrc)>
<!ELEMENT toolnbr       (#PCDATA |  acro | abbr)*              >
<!ELEMENT toolname      (#PCDATA |  acro | abbr)*              >
<!ELEMENT tooldesc      (#PCDATA |  acro | abbr)*              >
<!ELEMENT toolsrc       (#PCDATA)                              >


<!ELEMENT txtgrphc      (txtline+)                             >
<!ELEMENT txtline       (#PCDATA)                              >
<!--                                                         -->
