<!-- ========================================================= -->
<!--    REFERENCE BLOCK and GRAPHIC REFERENCING MECHANISMS     -->
<!-- ========================================================= -->
<!--      SEE HONEYWELL MOD 02, 03, 10                         -->
<!-- ********************************************************* -->

<!ELEMENT refblock  - - (#PCDATA | refint | refext | grphcref)*  >

<!ELEMENT grphcref  - - (effect?, (#PCDATA))                     >
<!ATTLIST grphcref
          refid     IDREF                         #REQUIRED
          sheetnbr  CDATA                         #IMPLIED
          structid  CDATA                         #IMPLIED
          shownow   NUMBER                        "0"            >

<!ELEMENT graphic   - - ((effect?, chgdesc*, title?, sheet+)
                         | (%deleted;))                          >
<!ATTLIST graphic
          chapnbr   NUMBER                        #REQUIRED
          sectnbr   NUMBER                        #REQUIRED
          subjnbr   NUMBER                        #REQUIRED
          func      CDATA                         " "
          seq       NUMBER                        #REQUIRED
          confltr   CDATA                         " "
          varnbr    NUMBER                        #REQUIRED
          pgblknbr  NUMBER                        #IMPLIED
          confnbr   NUMBER                        #IMPLIED
          %revatt;                                               >

<!ELEMENT sheet     - - ((effect?, chgdesc*, title?, gdesc?)
                         | (%deleted;))                          >
<!ATTLIST sheet
          gnbr          ENTITY                    #REQUIRED
          sheetnbr      CDATA                     #REQUIRED
          scalefit      (1|0)             "1"
          imgarea       (ap|bp|cp|dl|hl)  "ap"
          reprowid      (6.75in|8.00in|14.25in)      "6.75in"
          reprodep      (8in|6.750in|4in|2in)        "8in"
          %revatt; 
                                                        >

<!ELEMENT gdesc     - - (effect?, (%text;))                      >

<!-- ========================================================= -->
<!--              NUMBERED & UN-NUMBERED LISTS                 -->
<!-- ========================================================= -->

<!ELEMENT unlist    - - (unlitem+)                               >
<!ATTLIST unlist
          bulltype  (NONE, BULLET, NDASH, MDASH, 
                     DIAMOND, ASTERISK, DELTA, 
                     SQUARE, SYSTEM)              "SYSTEM"       >

<!ELEMENT unlitem   - - (para+)                                  >

<!ELEMENT numlist   - - (numlitem+)                              >
<!ATTLIST numlist
          numtype   (NNP, AUP, NNB, ALB, NNS, ALS, 
                     DBR, RUP, RLP, RUR, RLR, NNR, 
                     AUR, ALR)                    "DBR"          >

<!ELEMENT numlitem  - - (para+)                                  >
<!ATTLIST numlitem
          label     CDATA                         #IMPLIED       >

<!-- ========================================================= -->
<!--                    PARAGRAPH                              -->
<!-- ========================================================= -->

<!ELEMENT para      - - ((#PCDATA) | con | csn | equ | grphcref
                        | ncon | pnr | refext | refint | std 
                        | sub | super | ted | txtgrphc )+        >

<!-- ========================================================= -->
<!--                PARAGRAPH CONTENTS                         -->
<!-- ========================================================= -->
<!--      SEE HONEYWELL MOD 01, 06                             -->
<!-- ********************************************************* -->

<!ELEMENT con       - - (connbr, conname)                        >
<!ELEMENT connbr    - - (#PCDATA)                                >
<!ELEMENT conname   - - (#PCDATA)                                >

<!ELEMENT csn       - - (#PCDATA)                                >
<!ELEMENT equ       - - (#PCDATA | sub | super)+                 >
<!ELEMENT ncon      - - (#PCDATA)                                >

<!ELEMENT refext    - - (#PCDATA)                                >
<!ATTLIST refext
          refman    CDATA                         #IMPLIED
          docnbr    CDATA                         #IMPLIED
          refloc    CDATA                         #IMPLIED
          refspl    CDATA                         #IMPLIED
          refmodel  CDATA                         #IMPLIED       >

<!ELEMENT refint    - - (#PCDATA)                                >
<!ATTLIST refint
          reftype   CDATA                         #REQUIRED
          refid     IDREF                         #REQUIRED      >

<!ELEMENT std       - - (stdnbr?, stdname)                       >
<!ELEMENT stdnbr    - - (#PCDATA)                                >
<!ELEMENT stdname   - - (#PCDATA)                                >

<!ELEMENT sub       - - (#PCDATA)                                >
<!ELEMENT super     - - (#PCDATA)                                >


<!ELEMENT ted       - - (toolnbr, toolname)                      >
<!ELEMENT toolnbr   - - (#PCDATA)                                >
<!ELEMENT toolname  - - (#PCDATA)                                >

<!ELEMENT txtgrphc  - - (txtline+)                               >
<!ATTLIST txtgrphc
          type      (atlas, other, wl)     "other"               >

<!ELEMENT txtline   - - (#PCDATA)                                >                               


<!-- ========================================================= -->
<!--              TITLE, WARNING, CAUTION, NOTE, ETC           -->
<!-- ========================================================= -->

<!ELEMENT title     - - (#PCDATA | sub | super)+                 >
<!ELEMENT ltitle    - - (#PCDATA)                                >
<!ELEMENT warning   - - (effect?, (%text;)) -(note)              >
<!ELEMENT caution   - - (effect?, (%text;)) -(note)              >
<!ELEMENT note      - - (effect?, (%text;)) -(note)              >

<!-- ========================================================= -->
<!--               DEFINITION LIST CONSTRUCTS                  -->
<!-- ========================================================= -->

<!ELEMENT deflist   - - (title?, defdata+)                       >
<!ELEMENT defdata   - - (term, (def | (fullterm, def?)))         >
<!ELEMENT def       - - (#PCDATA)                                >
<!ELEMENT term      - - (#PCDATA)                                >
<!ELEMENT fullterm  - - (#PCDATA)                                >

<!-- ========================================================= -->
<!--               MISCELLANEOUS CONSTRUCTS                    -->
<!-- ========================================================= -->

<!ELEMENT chgdesc   - - (#PCDATA)                                >
<!ELEMENT deleted   - O  EMPTY                                   >
<!ELEMENT revst     - O  EMPTY                                   >
<!ELEMENT revend    - O  EMPTY                                   >
<!ELEMENT isempty   - O  EMPTY                                   >

<!ELEMENT effect    - - (#PCDATA)                                >

<!ELEMENT hotlink   - O  EMPTY                                   >
<!ATTLIST hotlink
          targetid     CDATA                      #IMPLIED 
          targetrefid  CDATA                      #IMPLIED       >
